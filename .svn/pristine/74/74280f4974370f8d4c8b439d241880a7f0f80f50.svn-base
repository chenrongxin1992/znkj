body {
  padding-top: 20px;
  padding-bottom: 20px;
  background-color: #0C0B0B;
  margin: 0 auto;
}

.navbar {
  border-bottom: 0px solid #0A0A0A;
  min-height: 20px;
}

.navbar-nav {
  margin-top: 0;
  margin-bottom: 0;
}

#carousel-banner {
  margin-bottom: -21px;
}

.carousel-caption {
  text-align: left;
  margin: 0 auto;
  left: 2%;
  padding-top: 0;
  top: 0;
}

.carousel-caption h1 {
  font-family: 'Quantico', sans-serif;
  font-size: 2.5em;
}

/*.navbar-brand {
  font-family: 'Quantico', sans-serif;
}*/

.caption-lead {
  font-family: 'Quantico', sans-serif;
  font-size: 1.3em;
}

.carousel-caption a:link, .carousel-caption a:hover, .carousel-caption a:visited, .carousel-caption a:active {
  color: #fff;
  text-decoration: none;
}

#nav-home {
  margin-left: -15px;
}

.nav {

}

.nav li a {
  border-right: 1px solid;
  border-right-color: #0A0A0A;
  min-width: 88px;
  text-align: center;
  padding-top: 8px;
  max-height: 38px;
}

.col-xs-6, .xs-sm-3 {
  padding-right: 0;
}

.row {
  background-color: #dbdbdb;
  margin-top: -20px;
  margin-left: 0;
  margin-right: 0;
}

.list-group-item {
  border: none;
}

.sidebar-title {
  font-weight: 700;
  font-family: 'Lato', sans-serif;
}

#logo-home {
  max-height: 150px;
  margin-bottom: -20px;
}

#logo-text {
  max-height: 150px;
  float: left;
}

.list-group-item-heading {
  font-weight: bold;
}
.list-group-item-text {
  font-style: italic;
  font-size: 1.2em;
}
.portrait {
  margin-left: -12px;
  float: left;
  margin-right: 5px;
}

#main-body-text h4 {
  font-size: 1.1em;
}

.not-show {
  visibility: hidden;
}

.dropdown-menu li a {
  border-right: 0;
}
li:hover ul.dropdown-menu {
  display: block;
}

.dropdown-menu li a {
  text-align: left;
}

h5 a {
  color: #000;
}

#real-title {
  font-size: 0;
}

.news {
  font-weight: 700;
  font-size: 15px;
}

.date-passed {
  text-decoration: line-through;
}

.alert {
  font-size: 15px;
}
.alert a {
  color: #fff;
  font-weight: bold;
}

.alert-success {
  background-color: #39AA54;
  border: none;
}

td {
  font-size: 1.2em;
}

.panel-heading {
  font-weight: bold;
}

.sec-head {
  font-family: 'Lato', sans-serif;
  font-size: 1.6em;
  font-weight: 700;
  margin-top: 50px;
  margin-bottom: 20px;
  border-bottom: 1px solid #cccccc
}

.speakers {
  text-align: left;
  margin-top: 15px;
  margin-left:22px;
}

.speakers strong {
  display: block;
  padding-top: 10px;
  padding-bottom: 5px;
  font-size: 17px;
}

.speakers ul {
  list-style-type: none;
  padding: 0;
}

.speakers ul a {
  color: #333;
}

.keynote-detail {
  /*display: block;*/
}

.keynotes .row {
  margin-top: 0;
}

.keynote-title {
  font-family: 'Lato', sans-serif;
  font-size: 18px;
  font-weight: 700;
  margin: 40px 0 2px 0;
}

.keynote-time {
  font-family: 'Lato', sans-serif;
  font-weight: 500;
  font-size: 16px;
  margin: 10px 0 10px 0; 
}

.space-spanner {
  min-width: 226px;
}

.bio-heading {
  font-weight: 700;
  font-size: 18px;
}