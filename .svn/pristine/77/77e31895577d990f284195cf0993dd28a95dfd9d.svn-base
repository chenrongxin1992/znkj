/************************     修改头像对话框样式       start    **************************/
.dialog-mask {
	border: 0px;
	background-color: rgb(0, 0, 0);
	margin: 0px;
	padding: 0px;
	position: absolute;
	top: 0px;
	left: 0px;
	opacity: .1;
	filter: alpha(opacity=10);
}
.dialog-main {
	background-color: #FFF;
	transition: left .4s linear, top .4s linear;
	-webkit-transition: left .4s linear, top .4s linear;
}
.dialog-header {
	height: 22px;
	line-height: 22px;
	padding: 23px 0 10px;
	background-color: #FFF;
	margin: 0 20px 10px;
	border-bottom: 1px solid #CCC;
	font-size: 14px;
	font-weight: bold;
	clear: both;
	text-align: left;
	position: relative;
}
.dialog-move {
	cursor: move;
}
.dialog-title {
	color: #3A3A3A;
	font-size: 16px;
	font-weight: normal;
	float: left;
	_display: inline;
}
.dialog-content {
	clear: both;
	overflow-y: hidden;
	overflow-x: hidden;
	font-size: 12px;
	background-color: #FFF;
	padding: 0 20px;
}
.dialog-close {
	background: url(/public/images/res_x_d.png) no-repeat scroll center center transparent;
	height: 16px;
	width: 16px;
	float: right;
	margin: 5px 0 0 0;
}
.avatar-upload-box {
	padding-bottom: 20px;
}
.avatar-upload-box .upload-field {
	margin-bottom: 18px;
}
.avatar-upload-box .upload-img-preview {
	padding: 0 8px 10px;
	display: none;
}
.clearfix {
	zoom: 1;
}
.avatar-upload-box .upload-msg.error, .sys-msg.error {
	border-color: #ee1e16;
	background-color: #fcd2d1;
	background-position: 6px -16px;
	color: #ee1e16;
}
.avatar-upload-box .upload-msg, .sys-msg {
	height: 26px;
	line-height: 26px;
	padding: 0 12px 0 30px;
	border: 1px dashed #54ae24;
	color: #54ae24;
	display: none;
}
.avatar-upload-box .upload-tips {
	height: 30px;
	line-height: 30px;
	color: #828282;
}
.avatar-upload-box .upload-msg, .sys-msg {
	height: 26px;
	line-height: 26px;
	padding: 0 12px 0 30px;
	border: 1px dashed #54ae24;
	color: #54ae24;
	display: none;
}
.avatar-upload-box {
	padding-bottom: 20px;
}
.upload-img-preview .original-img {
	float: left;
	border-right: 1px solid #ccc;
	padding-right: 40px;
	margin-top: 10px;
}
.upload-img-preview .original-img img {
	max-height: 300px;
	max-width: 300px;
}
.jcrop-tracker {
	-webkit-tap-highlight-color: transparent;
	-webkit-touch-callout: none;
	-webkit-user-select: none;
	padding: 6px!important;
	left: -8px!important;
	top: -7px!important;
	border: 1px solid rgba(0,0,0,0.4);
	border-radius: 6px 6px 6px 6px;
	box-shadow: 1px 1px 5px 2px rgba(0,0,0,0.2);
}
.upload-img-preview .preview-img {
	float: left;
	padding-left: 30px;
	text-align: center;
}
.upload-img-preview .preview-img .preview-img-holder {
	padding: 6px;
	margin: 4px 0 0 0;
	border: 1px solid rgba(0,0,0,0.4);
	border-radius: 6px 6px 6px 6px;
	box-shadow: 1px 1px 5px 2px rgba(0,0,0,0.2);
	overflow: hidden;
}
.upload-img-preview .preview-img .img-container {
	width: 150px;
	height: 220px;
	overflow: hidden;
}
.btn-save {
	height: 30px;
	line-height: 30px;
	padding: 0 15px;
	display: inline-block;
	margin-top: 20px;
	margin-right: 15px;
	color: #fff;
	font-weight: bold;
	background: rgb(114,165,82);
}
.btn-reset {
	height: 30px;
	line-height: 30px;
	padding: 0 15px;
	display: inline-block;
	margin-top: 20px;
	color: #7a7a7a;
	font-weight: bold;
	background: rgb(226,226,226);
}
.btn-common {
	margin: 0 20px 0 0;
	padding: 0 15px;
	display: inline-block;
	height: 23px;
	line-height: 23px;
	border: 1px solid #d2d2d2;
	background: #ffffff;
}
.avatar-upload-box .btn-common {
	margin-top: 20px;
	color: #878787;
	text-decoration: none;
	box-shadow: 1px 1px rgb(226, 226, 226);
}
.avatar-upload-box .btn-common:hover {background:#e2e2e2;}
table {width:100%;}

